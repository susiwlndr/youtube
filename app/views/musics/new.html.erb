<div class="card">
  <div class="card-header">
    Upload Video Music Anda
  </div>
  <div class="card-body">

    <%= form_for @music do |f| %>
    <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control' %>
    <%= f.label :description %>
    <%= f.text_area :description, rows: 3, class: 'form-control' %>
    </div>
    <%= f.submit "Upload", class: 'btn btn-primary' %>
    <% end %>
  </div> 
</div>